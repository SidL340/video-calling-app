{
  "name": "video-calling-app",
  "version": "1.0.0",
  "description": "A fully working video calling app with user registration, text chat, and file sharing",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
